{
  "name": "flow-view",
  "description": "is a visual editor for dataflow programming",
  "version": "5.5.0",
  "engines": {
    "node": ">=14",
    "deno": ">=1.10"
  },
  "type": "module",
  "main": "flow-view.js",
  "license": "MIT",
  "homepage": "http://fibo.github.io/flow-view",
  "author": {
    "name": "Gianluca Casati",
    "url": "http://fibo.github.io"
  },
  "scripts": {
    "build": "deno bundle main.js | esbuild --minify --target=safari11 > flow-view.js",
    "fmt": "deno fmt --ignore=./flow-view.js",
    "lint": "deno lint --ignore=./flow-view.js",
    "postversion": "git push origin v${npm_package_version}; git push origin main; npm publish",
    "start": "serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/flow-view.git"
  },
  "keywords": [
    "dataflow",
    "visual",
    "editor",
    "vanilla"
  ],
  "bugs": {
    "url": "https://github.com/fibo/flow-view/issues"
  },
  "dependencies": {}
}
